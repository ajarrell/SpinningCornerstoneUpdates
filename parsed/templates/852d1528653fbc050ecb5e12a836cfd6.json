{"components/blog/slide":"<div class=\"card\">\r\n    <div class=\"card-outer\">\r\n        {{#if post.thumbnail}}\r\n        <div class=\"card-figure\">\r\n                <figure class=\"card-image\">\r\n                    <a href=\"{{post.url}}\">\r\n                        <img src=\"{{getImage post.thumbnail 'blog_size'}}\" alt=\"{{post.thumbnail.alt}}\">\r\n                    </a>\r\n                </figure>\r\n        </div>\r\n        {{/if}}\r\n        <div class=\"card-inner\">\r\n            <div class=\"card-title\">\r\n                <h2 class=\"blog-title\">\r\n                    <a href=\"{{post.url}}\">{{post.title}}</a>\r\n                </h2>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\n"}
