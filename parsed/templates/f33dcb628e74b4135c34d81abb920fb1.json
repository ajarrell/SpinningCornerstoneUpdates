{"components/common/cta-arrow":"<a href=\"{{#if link}}{{link}}{{else}}#{{/if}}\"\n   class=\"cta-arrow{{#if border_top_color}} showTopBorder{{/if}}\"\n   style=\"border-top-color: {{#if border_top_color}}{{border_top_color}}{{else}}#fff{{/if}}\"\n   {{#if scroll_to}}data-scroll-to=\"{{scroll_to}}\"{{/if}}>\n    <span class=\"cta-arrow--featured-link-text\"\n          style=\"color:{{#if text_color}}{{text_color}}{{else}}#333{{/if}}\">\n        {{#if text}}\n            {{text}}\n        {{else}}\n            Learn More\n        {{/if}}\n    </span>\n    <span class=\"cta-arrow--featured-link-arrow\">\n        {{#if arrow}}\n            {{{arrow}}}\n        {{else}}\n            {{#if white_arrow}}\n                <img src=\"{{cdn 'assets/img/icons/cta-arrow-white.png'}}\">\n            {{else}}\n                <img src=\"{{cdn 'assets/img/icons/cta-arrow.png'}}\">\n            {{/if}}\n        {{/if}}\n    </span>\n</a>\n","components/common/slider-with-feature":"<div class=\"slider-with-feature\">\n    <h2 class=\"section-header\">{{{title}}}</h2>\n    <p class=\"section-description\">{{{description}}}</p>\n\n    <div class=\"slider-with-feature--inner\">\n        <div class=\"gradient fromOrange toRed\">\n            <div class=\"row\">\n                <div class=\"col-sm-6\">\n                    <img class=\"slider-with-feature--featured-image\" src=\"{{cdn slider_1_image}}\">\n                </div>\n                <div class=\"col-sm-6\">\n                    <div class=\"slider-with-feature--content\">\n                        <h3 class=\"slider-with-feature--featured-title\">\n                            {{{slider_1_title}}}\n                        </h3>\n                        <p class=\"slider-with-feature--featured-description\">\n                            {{{slider_1_description}}}\n                        </p>\n                        <div class=\"slider-with-feature--featured-link\">\n                            {{>\n                                components/common/cta-arrow\n                                link=slider_1_link\n                                text_color='#fff'\n                                white_arrow=true\n                                border_top_color='#fff'\n                            }}\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n\n        <div class=\"row\">\n            <div class=\"col-md-6\"></div>\n            <div class=\"col-md-6\">\n                <ul class=\"slider-with-feature--slider noselect\">\n                    <i class=\"fa fa-arrow-circle-left\"></i>\n                    <li>\n                        <img class=\"active\"\n                             src=\"{{cdn slider_1_image}}\"\n                             data-featured-title=\"{{slider_1_title}}\"\n                             data-featured-description=\"{{slider_1_description}}\"\n                             data-featured-link=\"{{slider_1_link}}\"\n                             width=\"50\">\n                    </li>\n                    <li>\n                        <img src=\"{{cdn slider_2_image}}\"\n                             data-featured-title=\"{{slider_2_title}}\"\n                             data-featured-description=\"{{slider_2_description}}\"\n                             data-featured-link=\"{{slider_2_link}}\"\n                             width=\"50\">\n                    </li>\n                    <li>\n                        <img src=\"{{cdn slider_3_image}}\"\n                             data-featured-title=\"{{slider_3_title}}\"\n                             data-featured-description=\"{{slider_3_description}}\"\n                             data-featured-link=\"{{slider_3_link}}\"\n                             width=\"50\">\n                    </li>\n                    <i class=\"fa fa-arrow-circle-right\"></i>\n                </ul>\n            </div>\n        </div>\n    </div>\n</div>\n"}
